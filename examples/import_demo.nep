aayaat "examples/string_utils.nep"

bhan "Math Functions haru test:"
bhan "========================"

maanau num = 5
bhan "Number:"
bhan num

bhan "Square:"
bhan square(num)

bhan "Cube:" 
bhan cube(num)

bhan "Factorial:"
bhan factorial(num)

bhan "Even cha ki:"
bhan is_even(num)

bhan "Odd cha ki:"
bhan is_odd(num)

maanau a = 10
maanau b = 7
bhan "Max"
bhan a
bhan "ra"  
bhan b
bhan "ko beech ma:"
bhan max(a, b)

bhan "Min chai:"
bhan min(a, b)

bhan "PI constant:"
bhan PI

bhan ""
