bhan "=========================="

maanau test_word = "Kathmandu"
bhan "Test word:"
bhan test_word

bhan "Length:"
bhan string_length(test_word)

bhan "Vowel count:"
bhan count_vowels(test_word)

bhan "Greeting constant:"
bhan GREETING

bhan "Farewell constant:"
